{
  "name": "sap",
  "version": "2.0.0",
  "description": "Sistema de Apoio a Produção",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/1cgeo/sap"
  },
  "scripts": {
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "start-all": "concurrently \"cd server && npm run dev\" \"cd client && npm run start\"",
    "config": "node create_config.js"
  },
  "nodemonConfig": {
    "ignore": [
      "src/js_docs/*"
    ]
  },
  "keywords": [
    "Controle de Produção",
    "Node",
    "Express"
  ],
  "author": "DSG/1CGEO <diniz.felipe@eb.mil.br>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1cgeo/sap/issues"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "colors": "^1.4.0",
    "concurrently": "^5.0.2",
    "inquirer": "^7.0.1",
    "pg-promise": "^10.3.1",
    "pgtools": "^0.3.0"
  }
}
